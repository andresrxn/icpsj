{
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "singleAttributePerLine": false,
  "quoteProps": "as-needed",
  "bracketSameLine": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 100,
  "endOfLine": "crlf",
  "plugins": ["prettier-plugin-tailwindcss"]
}
